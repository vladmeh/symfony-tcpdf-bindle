{
  "name": "vladmeh/tcpdf-bundle",
  "version": "3.0.1",
  "type": "symfony-bundle",
  "description": "A bundle to easily integrate TCPDF into Symfony4",
  "keywords": ["pdf", "tcpdf", "symfony"],
  "homepage": "https://github.com/vladmeh/symfony-tcpdf-bindle",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Mikhaylov",
      "email": "vladmeh@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/framework-bundle": "^4.0",
    "tecnickcom/tcpdf": "^6.2"
  },
  "autoload": {
    "psr-4": { "Vladmeh\\Bundle\\TCPDFBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "Vladmeh\\Bundle\\TCPDFBundle\\Tests\\": "Tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "symfony/phpunit-bridge": "^4.0"
  }
}
